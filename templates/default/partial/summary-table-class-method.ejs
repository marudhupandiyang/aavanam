<div class="method">
  <h4>
    <span class="access <%= access %>">
      <%= access %>
    </span>
    <%= title %>
    (<%- params.map(p => {
      const v = [`<span class="param">${p.name}</span>`];
      if (tags && tags.params && tags.params[p.name]) {
        v.push(`:&nbsp;${view.getTypeWithReferenceLink(tags.params[p.name].type)}`);
      }
      return v.join('');
    }) %>)
  </h4>
  <div class="desc">
    <% if (tags.subHeading || tags.description) { %>
      <%- tags.subHeading %>
      <%- (tags.subHeading ? '<br />' : '') %>
      <%- tags.description %>
    <% } %>
  </div>
</div>
