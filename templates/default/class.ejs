<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8"/>
    <title><%= pageTitle %></title>
    <link rel="stylesheet" type="text/css" href="/public/style.css">
  </head>
  <body class="class-detail">
    <%- include('partial/header'); %>
    <div class="body-container">
      <%- include('partial/menusidebar') %>
      <div class="content">
        <div class="breadcrumb"><%= currentClass.parent %></div>
        <h2 class="class-name"><%= currentClass.name %></h2>
        <% if (currentClass.extends) { %>
          <div class="extends">
            <div class="title">Extends:</div>
            <div class="content">
              <% currentClass.extends.forEach(function(e) { %>
                <span><%= e %></span>
              <% }) %>
            </div>
          </div>
        <% } %>
        <p class="sub-heading">
          <%= currentClass.subHeading %>
        </p>
        <p class="description">
          <%= currentClass.description %>
        </p>

        <% if (currentClass.properties && currentClass.properties.length > 0) { %>
        <h3>Class Properties: </h3>
          <% for( var i = 0; i < currentClass.properties.length; i +=1 ) { %>
              <%- include('partial/summary-table-class-property', {
                access: 'public',
                title: currentClass.properties[i].name,
                // params: currentClass.properties[i].params,
                // tags: currentClass.properties[i].tags,
              }) %>
          <% }%>
        <% } %>

        <% if (currentClass.methods && currentClass.methods.length > 0) { %>
        <h3>Class Methods: </h3>
          <% for( var i = 0; i < currentClass.methods.length; i +=1 ) { %>
              <%- include('partial/summary-table-class-method', {
                access: 'public',
                title: currentClass.methods[i].name,
                params: currentClass.methods[i].params,
                tags: currentClass.methods[i].tags,
              }) %>
          <% }%>
        <% } %>
      </div>
    </div>
    <%- include('partial/footer') %>
  </body>
</html>
